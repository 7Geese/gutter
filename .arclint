{
  "linters" : {
    "flake8" : {
      "type" : "flake8",
      "include" : "(\\.py$)",
      "exclude" : "(^.git/|^ci/|^env/|/vendor/|_pb2\\.py$)",
      "severity.rules" : {
        "(^(W292|W391))" : "disabled",
        "(^E501)" : "warning"
      }
    },
    "filename" : {
      "type" : "filename",
      "include" : "(.*)"
    },
    "text" : {
      "type" : "text",
      "include" : "(\\.txt$)"
    },
    "nolint" : {
      "type" : "nolint",
      "include" : "(.*)"
    },
    "generated" : {
      "type" : "generated",
      "include" : "(.*)"
    }
  }
}
